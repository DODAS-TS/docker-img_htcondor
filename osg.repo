[osg]
name=OSG Software for Enterprise Linux 7 - $basearch
#baseurl=https://repo.opensciencegrid.org/osg/3.4/el7/release/$basearch
mirrorlist=https://repo.opensciencegrid.org/mirror/osg/3.4/el7/release/$basearch
failovermethod=priority
priority=98
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-OSG

[osg-source]
name=OSG Software for Enterprise Linux 7 - $basearch - Source
baseurl=https://repo.opensciencegrid.org/osg/3.4/el7/release/source/SRPMS
failovermethod=priority
priority=98
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-OSG

[osg-debuginfo]
name=OSG Software for Enterprise Linux 7 - $basearch - Debug
baseurl=https://repo.opensciencegrid.org/osg/3.4/el7/release/$basearch/debug
failovermethod=priority
priority=98
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-OSG

